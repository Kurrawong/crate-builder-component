        "@element-plus/theme-chalk": "^2.2.16",
        "@fortawesome/fontawesome-free": "^6.2.0",
        "@fortawesome/fontawesome-svg-core": "^6.2.0",
        "codemirror": "^6.0.1",
        "date-fns": "^2.29.3",
        "element-plus": "^2.2.17",
        "leaflet": "^1.9.1",
        "leaflet-area-select": "^1.0.5",
        "lodash": "^4.17.21",
        "tailwindcss": "^3.1.8",
        "uuid": "^9.0.0",
        "validator": "^13.7.0",
        "vue": "^3.2.40",
        "vue-router": "^4.1.5",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.18.9",
