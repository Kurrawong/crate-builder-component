{
    "name": "@describo/crate-builder-component",
    "version": "0.23.1",
    "description": "A VueJS component for assembling an RO-Crate",
    "main": "./src/crate-builder/index.js",
    "files": [
        "dist",
        "src/crate-builder"
    ],
    "scripts": {
        "develop": "npm install && npm run start:elastic && vite  && npm run stop:elastic",
        "test:watch": "./node_modules/.bin/jest --watch --runInBand",
        "build": "npm run build:vue && npm run build:wc",
        "build:vue": "vite build",
        "build:wc": "npx rollup -c && vite --config vite.wc.config.js build",
        "start:elastic": "docker compose up -d && npm run load:datapacks",
        "load:datapacks": "sleep 10 && node ./load-data-packs.js",
        "stop:elastic": "docker compose stop ; docker compose rm -f",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/describo/crate-builder-component.git"
    },
    "author": "Marco La Rosa",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/describo/crate-builder-component/issues"
    },
    "homepage": "https://github.com/describo/crate-builder-component#readme",
    "dependencies": {
        "@codemirror/lang-javascript": "^6.1.2",
        "@codemirror/state": "^6.2.0",
        "@codemirror/theme-one-dark": "^6.1.0",
        "@coedl/elastic-query-builder": "^0.3.0",
        "@describo/data-packs": "^0.10.0",
        "@element-plus/theme-chalk": "^2.2.16",
        "@fortawesome/fontawesome-free": "^6.2.1",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "codemirror": "^6.0.1",
        "date-fns": "^2.29.3",
        "element-plus": "^2.2.28",
        "leaflet": "^1.9.3",
        "leaflet-area-select": "^1.0.5",
        "lodash": "^4.17.21",
        "tailwindcss": "^3.2.4",
        "uuid": "^9.0.0",
        "validator": "^13.9.0",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@storybook/addon-actions": "^7.0.0-beta.46",
        "@storybook/addon-docs": "^6.5.16",
        "@storybook/addon-essentials": "^7.0.0-beta.46",
        "@storybook/addon-interactions": "^7.0.0-beta.46",
        "@storybook/addon-links": "^7.0.0-beta.46",
        "@storybook/testing-library": "^0.0.14-next.1",
        "@storybook/vue3": "^7.0.0-beta.46",
        "@storybook/vue3-vite": "^7.0.0-beta.46",
        "@vitejs/plugin-vue": "^4.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^8.3.0",
        "chance": "^1.1.9",
        "cssnano": "^5.1.14",
        "jest": "^29.3.1",
        "postcss": "^8.4.21",
        "process": "^0.11.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "regenerator-runtime": "^0.13.11",
        "rollup": "^3.14.0",
        "rollup-plugin-copy": "^3.4.0",
